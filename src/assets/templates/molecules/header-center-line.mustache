{{#mol-header-center-line}}
    <div class="center-line-container-desc">
        <div class="container d-flex justify-content-between align-items-center">
            <div class="center-line-container-desc__left-side-info d-flex align-items-center">
                <a href="mailto:akr.balans@gmail.com" class="mr-60"><img src="{{{email-icon}}}" alt="img"/>{{email}}</a>
                <p><img src="{{{location-icon}}}" alt="img" />{{location}}</p>
            </div>
            <div class="center-line-container-desc__right-side-info d-flex align-items-center">
                <div class="socials mr-15">
                    <a href="{{{whatsapp-url}}}"><img src="{{{whatsapp-icon}}}"/></a>
                    <a href="{{{viber-url}}}"><img src="{{{viber-icon}}}"/></a>
                    <a href="{{{telegram-url}}}"><img src="{{{telegram-icon}}}"/></a>
                </div>
                <div class="phone-info">
                    <p><img src="{{{mobile-phone-icon-url}}}" alt="img" />{{mobile-phone}}</p>
                </div>
            </div>
        </div>
    </div>
{{/mol-header-center-line}}
